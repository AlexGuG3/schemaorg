<!-- Properties for Term: {{propertiesFor.id}}-->

{% for prop in propertiesFor.properties %}
    {% if loop.first %}
        <tr class="supertype"><td class="supertype-name {% if tabletop %}tabletop{% endif %}" colspan="3">Properties from {{ sdotermlink(propertiesFor)|safe }}</td></tr>
        <tr class="props-hdr">
            <td class="props-hdr-nam">Property</td>
            <td class="props-hdr-ect">Expected Type</td>
            <td class="props-hdr-desc">Description</td></tr>
    {% endif %}
	{% if not prop.retired and not prop.superseded %}
        {% if loop.first %}
            <tr class="prop-row-first">
        {% else %}
            <tr class="prop-row-rest">
        {% endif %}
            <th class="prop-nam"><code>{{ sdotermlink(prop)|safe }}</code></th>
            <td class="prop-ect">{% for ect in prop.rangeIncludes %}{% if not loop.first %}&nbsp; or <br/>{% endif %}
                {{ sdotermlink(ect)|safe }}{% endfor %}</td>
            <td class="prop-desc">{{ prop.comment | safe}}
                {% for ss in prop.supersedes %}{% if loop.first %} Supersedes {% else %}, {% endif %}
                {{ sdotermlink(ss)|safe }}{% if loop.last%}.{% endif %}{% endfor %}
                {% if prop.supersededBy %} SupersededBy {{sdotermlink(prop.supersededBy)| safe}}. {%endif %}
                {% if prop.inverse %}<br/>Inverse property: {{ sdotermlink(prop.inverse)|safe }}{% endif %}
            </td>
        </tr>
    {% endif %}
{% endfor %}
