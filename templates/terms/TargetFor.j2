{% for prop in term.expectedTypeFor %}
  {% if loop.first %}
	<br/><div id="incoming" class="jumptarget" title="Value for">Instances of {{ sdotermlink(term.id)|safe}} {{INSERT}} may appear as a value for the following properties</div>
	<table class="definition-table">
          <tr class="props-hdr">
            <td class="props-hdr-nam tabletop-left">Property</td>
            <td class="props-hdr-ect table-top-center">On Types</td>
            <td class="props-hdr-desc tabletop-right">Description</td></tr>

  {% endif %}
    {% if loop.first %}
        <tr class="prop-row-first">
    {% else %}
        <tr class="prop-row-rest">
    {% endif %}

      <th class="prop-nam">{{ sdotermlink(prop.id)|safe}}</th>
      <td class="prop-ect">{% for ect in prop.domainIncludes %}{% if not loop.first %}&nbsp; or <br/>{% endif %}{{ sdotermlink(ect)|safe}}{% endfor %}</td>
      <td class="prop-desc">{{ prop.comment | safe}}
  </tr> 
  
  {% if loop.last %}
	 </table>
  {% endif %}
{% endfor %}
